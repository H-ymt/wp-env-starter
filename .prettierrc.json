{
 "plugins": ["@prettier/plugin-php"],
  "arrowParens": "always",
  "printWidth": 150,
  "tabWidth": 2,
  "singleQuote": false,
  "useTabs": false,
  "semi": true,
  "trailingComma": "es5",
  "overrides": [
    {
      "files": "*.json",
      "options": {
        "tabWidth": 2
      }
    }
  ]
}
